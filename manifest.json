{
    "name": "Expediente",
    "description": "Script para calculo de expediente",
    "background": {
        "scripts": [
            "ext.js"
        ]
    },
    "version": "1.0",
    "manifest_version": 2,
    "browser_action": {
        "default_title": "Click Me",
        "default_icon": "icon.png",
        "default_popup": "ext.html"
    },
    "icons": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
    },
    "permissions": [
        "tabs", "<all_urls>"
    ]
}